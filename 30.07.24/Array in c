#include <stdio.h>

int main() {
    int arr[] = {25, 11, 7, 75, 56};
    int n = sizeof(arr) / sizeof(arr[0]);
    int largest = arr[0];

    for(int i = 1; i < n; i++) {
        if(arr[i] > largest) {
            largest = arr[i];
        }
    }

    printf("Largest element in the array is %d\n", largest);
    return 0;
}
